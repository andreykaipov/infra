        AZURE_SUBSCRIPTION_ID    = local.secrets.azure.subscription_id
        AZURE_RESOURCE_GROUP     = dependency.rg.outputs.name
        AZURE_CONTAINER_APP_NAME = "minecraft-java"
      }
    }
  ]
}
